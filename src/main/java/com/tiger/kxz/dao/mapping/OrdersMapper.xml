<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tiger.kxz.dao.OrdersMapper">
	<resultMap id="BaseResultMap"
		type="com.tiger.kxz.entity.Orders">
		<id column="ORDER_NO" property="ORDER_NO" jdbcType="VARCHAR" />
		<result column="GEN_INSTTU_CODE" property="GEN_INSTTU_CODE"
			jdbcType="VARCHAR" />
	</resultMap>
	<select id="selectByPrimarykey" resultType="java.lang.String" resultMap="BaseResultMap">
		SELECT * FROM orders WHERE order_no = #{id,jdbcType=VARCHAR};
	</select>

</mapper>